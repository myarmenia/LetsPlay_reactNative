{
  "name": "gameorganizing",
  "version": "1.0.5",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint --quiet --ext .js --fix src",
    "pod": "cd ios && pod install && cd ..",
    "ios14": "react-native run-ios --simulator='iPhone 14'",
